  <table>
  
  <tr>
    <th>Status</th><th>Order No</th>
    <th>Order Date</th>
    <th>First Name</th><th>Last Name</th>
   <th># of Items</th>
   <th>Product Title(s)</th>
  </tr>
  <%@clients.each do |client|%>
      <tr>
        <td><%=client.status%></td>
          <td><%=client.order_no%></td>
          <td><%=client.order_date%></td>
          <td><%=client.billing_first_name%></td>
          <td><%=client.billing_name%></td>
          <td><%=@numItemsInOrder[client.order_no]%></td>
          <td><%=client.product_title%></td>
      </tr>
  <%end%>

</table>
